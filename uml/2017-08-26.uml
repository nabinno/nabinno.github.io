@startuml
skinparam monochrome true
skinparam backgroundColor #EEEEFF

node "pool" {
  [ppool_sup] --> [ppool_serv]
  [ppool_sup] --> [worker_sup]
  [worker_sup] --> [worker_1]
  [worker_sup] --> [worker_2]
  [worker_sup] --> [worker_3]
}

[ppool_supersup] --> [ppool_sup]
[other_pool] --> [ppool_supersup]

@enduml
